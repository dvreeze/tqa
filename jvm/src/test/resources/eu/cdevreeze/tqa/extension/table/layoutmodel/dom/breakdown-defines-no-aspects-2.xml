<?xml version='1.0' encoding='utf-8'?>
<tableModel xmlns="http://xbrl.org/2014/table/model" xml:base="..">
  <tableSet>
    <label>Breakdowns with and without constraints on an axis</label>
    <table>
      <headers axis="y">
        <group>
          <label>Two rows (primary items m1, m2)</label>
          <header>
            <cell>
              <label>Label: m1</label>
              <label>1</label>
              <constraint>
                <aspect>concept</aspect>
                <value xmlns:rend="http://www.xbrl.org/table-examples">rend:m1</value>
              </constraint>
            </cell>
            <cell>
              <label>Label: m2</label>
              <label>2</label>
              <constraint>
                <aspect>concept</aspect>
                <value xmlns:rend="http://www.xbrl.org/table-examples">rend:m2</value>
              </constraint>
            </cell>
          </header>
        </group>
      </headers>
      <headers axis="x">
        <group>
          <label>Two columns (dimension D = d1, d2)</label>
          <header>
            <cell>
              <label>Label: D = d1</label>
              <label>1</label>
              <constraint>
                <aspect xmlns:rend="http://www.xbrl.org/table-examples">rend:D</aspect>
                <value xmlns:rend="http://www.xbrl.org/table-examples">rend:d1</value>
              </constraint>
            </cell>
            <cell>
              <label>Label: D = d2</label>
              <label>2</label>
              <constraint>
                <aspect xmlns:rend="http://www.xbrl.org/table-examples">rend:D</aspect>
                <value xmlns:rend="http://www.xbrl.org/table-examples">rend:d2</value>
              </constraint>
            </cell>
          </header>
        </group>
        <group>
          <label>Single column (no constraint)</label>
          <header>
            <cell>
              <label>No constraint</label>
            </cell>
            <cell>
              <label>No constraint</label>
            </cell>
          </header>
        </group>
      </headers>
      <cells axis="z">
        <cells axis="y">
          <cells axis="x">
            <cell>
              <fact>breakdown-defines-no-aspects-2-instance.xml#m1_D_d1</fact>
            </cell>
            <cell>
              <fact>breakdown-defines-no-aspects-2-instance.xml#m1_D_d2</fact>
            </cell>
          </cells>
          <cells axis="x">
            <cell>
              <fact>breakdown-defines-no-aspects-2-instance.xml#m2_D_d1</fact>
            </cell>
            <cell>
              <fact>breakdown-defines-no-aspects-2-instance.xml#m2_D_d2</fact>
            </cell>
          </cells>
        </cells>
      </cells>
    </table>
  </tableSet>
</tableModel>
